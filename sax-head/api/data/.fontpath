# check basic font path according to Xorg v6 and v7
#
rpm -q xorg-x11-server-sdk >/dev/null 2>&1
if [ ! $? = 0 ];then
	exit 0
fi
file=./StaticFontPathList
cat $file | sed -e s@usr/lib/X11@usr/share@ > $file.new
mv $file.new $file
