#-------------------------------#
# Wacom Intuos tablet USB       #
#-------------------------------#
WACOM:INTUOS v1-3 (USB) {
 TabletType       = cursor
 TabletMode       = Absolute
 Driver           = wacom
 Device           = /dev/input/wacom
 Option           = Tilt,USB
 SendCoreEvents   = on
 KernelLoad       = wacom
 StylusLink       = INTUOS v1-3 Stylus (USB)
 EraserLink       = INTUOS v1-3 Eraser (USB)
}

