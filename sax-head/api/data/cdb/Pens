#-------------------------------#
# Wacom Intuos Eraser USB       #
#-------------------------------#
WACOM:INTUOS v1-3 Eraser (USB) {
 Device              = /dev/input/wacom
 TabletMode          = Absolute
 Driver              = wacom
 Protocol            = Auto
 TabletType          = eraser
 Option              = USB,Tilt
 SendCoreEvents      = on
 ButtonThreshold     = 5
}

#-------------------------------#
# Wacom Intuos Stylus USB       #
#-------------------------------#
WACOM:INTUOS v1-3 Stylus (USB) {
 Device              = /dev/input/wacom
 TabletMode          = Absolute
 Driver              = wacom
 Protocol            = Auto
 TabletType          = stylus
 Option              = USB,Tilt
 ButtonThreshold     = 5
 SendCoreEvents      = on
}

