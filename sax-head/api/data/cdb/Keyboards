 XkbModel             = pc105
 XkbLayout            = gb
 AutoRepeat           = 500 30
 ScrollLock           = Compose
 RightCtl             = Control 
 LeftAlt              = Meta  
 RightAlt             = Meta
 ApplyKeymap          = xfree86
 ApplyKeycodes        = xfree86
 ApplyCompat          = complete
 ApplyTypes           = complete
 ApplySymbols         = en_US(pc105)
 ApplyGeometry        = pc(pc105)
}

