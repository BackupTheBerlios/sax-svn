#----------------------------------#
# all keys as a sample             #
#----------------------------------#
#all:<...> {
#  Protocol             = Xqueue
#  Driver               = keyboard
#  XkbRules             = xfree86
#  XkbModel             = pc105
#  XkbLayout            = gb
#  XkbVariant           = nodeadkeys
#  XkbOptions           = ctrl:swapcaps,grp:switch
#  AutoRepeat           = 200 100
#  Xleds                = 1 3
#  XkbDisable           = <yes>
#  VTSysReq             = <no>
#  VTInit               = blubber
#  ServerNumLock        = <yes>
#  LeftAlt              = Meta
#  RightAlt             = Compose
#  ScrollLock           = ModeShift
#  RightCtl             = ModeLock
#  XkbCompat            = blubber
#}

