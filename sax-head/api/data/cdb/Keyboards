#
# Template for keyboard data
# ---------------------------
# <model>:<comment> {
#  Protocol             = protocol... ( Standard | Xqueue )
#  Driver               = driver ( keyboard )
#  XkbRules             = rules ( xfree86 )
#  XkbModel             = model data 
#  XkbKeyCodes          = ( xfree86 , macintosh )
#  XkbLayout            = language layout ( de, en ... )
#  XkbVariant           = nodeadkeys
#  XkbOptions           = options ( ctrl:swapcaps,grp:switch )     
#  AutoRepeat           = delay rate
#  Xleds                = led use ( 1 2 3 )
#  XkbDisable           = <yes> | <no>
#  VTSysReq             = <yes> | <no>
#  VTInit               = string
#  ServerNumLock        = <yes> | <no>
#  LeftAlt              = [ Meta Compose ModeShift ModeLock ScrollLock Control ]
#  RightAlt             = [ Meta Compose ModeShift ModeLock ScrollLock Control ]
#  ScrollLock           = [ Meta Compose ModeShift ModeLock ScrollLock Control ]
#  RightCtl             = [ Meta Compose ModeShift ModeLock ScrollLock Control ]
#  XkbCompat            = string
#  XkbKeymap            = string
#  ApplyKeymap          = keymap for apply call
#  ApplyKeycodes        = keycodes for apply call
#  ApplyCompat          = compat for apply call
#  ApplyTypes           = types for apply call
#  ApplySymbols         = symbols for apply call
#  ApplyGeometry        = geometry for apply call
# }

#-------------------------------#
# generic us keyboard           #
#-------------------------------#
Generic keyboard [ pc101 ]: <...> {
 Protocol             = Standard
 Driver               = keyboard
 XkbRules             = xfree86
 XkbModel             = pc101
 XkbLayout            = us
 ApplyKeymap          = xfree86
 ApplyKeycodes        = xfree86
 ApplyCompat          = complete
 ApplyTypes           = complete
 ApplySymbols         = en_US(pc101)
 ApplyGeometry        = pc(pc101)
}

#---------------------------------#
# generic Sun Keyboard (Type 5)   #
#---------------------------------#
Sun Keyboard [ type5_euro ]: <...> {
 Protocol             = Standard
 Driver               = keyboard
 XkbRules             = sun
 XkbModel             = type5_euro
 XkbKeyCodes          = sun
 XkbLayout            = de
 ApplyKeymap          = xfree86
 ApplyKeycodes        = sun(type5)
 ApplyCompat          = complete
 ApplyTypes           = complete
 ApplySymbols         = sun/us(sun5)
 ApplyGeometry        = sun(type5euro)
}

