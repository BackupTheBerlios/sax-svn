InputDevice -> 1 -> Identifier = Mouse[1]
InputDevice -> 1 -> Driver     = synaptics
InputDevice -> 1 -> Option -> InputFashion         = Mouse
InputDevice -> 1 -> Option -> Name                 = ALPS;Touchpad
InputDevice -> 1 -> Option -> Device               = /dev/input/mice
InputDevice -> 1 -> Option -> ZAxisMapping         = 4 5
InputDevice -> 1 -> Option -> Emulate3Buttons      = on
InputDevice -> 1 -> Option -> SHMConfig            = on
InputDevice -> 1 -> Option -> Protocol             = auto-dev
InputDevice -> 1 -> Option -> LeftEdge             = 120
InputDevice -> 1 -> Option -> RightEdge            = 830
InputDevice -> 1 -> Option -> TopEdge              = 120
InputDevice -> 1 -> Option -> BottomEdge           = 650
InputDevice -> 1 -> Option -> FingerLow            = 14
InputDevice -> 1 -> Option -> FingerHigh           = 15
InputDevice -> 1 -> Option -> MaxTapTime           = 180
InputDevice -> 1 -> Option -> MaxTapMove           = 110
InputDevice -> 1 -> Option -> EmulateMidButtonTime = 75
InputDevice -> 1 -> Option -> VertScrollDelta      = 20
InputDevice -> 1 -> Option -> HorizScrollDelta     = 20
InputDevice -> 1 -> Option -> MinSpeed             = 0.2
InputDevice -> 1 -> Option -> MaxSpeed             = 0.5
InputDevice -> 1 -> Option -> AccelFactor          = 0.01
InputDevice -> 1 -> Option -> EdgeMotionMinSpeed   = 15
InputDevice -> 1 -> Option -> EdgeMotionMaxSpeed   = 15
InputDevice -> 1 -> Option -> UpDownScrolling      = 1
InputDevice -> 1 -> Option -> CircularScrolling    = 1
InputDevice -> 1 -> Option -> CircScrollDelta      = 0.1
InputDevice -> 1 -> Option -> CircScrollTrigger    = 2
